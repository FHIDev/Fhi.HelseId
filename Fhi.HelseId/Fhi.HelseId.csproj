<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Nullable>enable</Nullable>
    <LangVersion>8</LangVersion>
    <Configurations>Debug;Release</Configurations>
    <AssemblyVersion>1.2.1.0</AssemblyVersion>
    <FileVersion>1.2.1.0</FileVersion>
    <Description>FHI sin integrasjon med HelseId. Til bruk i .netcore3.1 WebAPI-prosjekter. </Description>
    <Version>1.2.1</Version>
    <PackageReleaseNotes>Whitelist</PackageReleaseNotes>
    <RepositoryUrl>https://fhi.visualstudio.com/DefaultCollection/Fhi.Msis/_git/Fhi.HelseId</RepositoryUrl>
    <dependencies>
      <group targetFramework="netcoreapp3.1">
      </group>
    </dependencies>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Web\HprServiceReference\ConnectedService.json" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Web\HprServiceReference\ConnectedService.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <ExcludeFromSingleFile>true</ExcludeFromSingleFile>
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="IdentityServer4.AccessTokenValidation" Version="3.0.1" />
    <PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="3.1.2" />
    <PackageReference Include="Microsoft.AspNetCore.Authentication.OpenIdConnect" Version="3.1.2" />
    <PackageReference Include="Microsoft.AspNetCore.Components.Analyzers" Version="3.1.3" />
    <PackageReference Include="System.ServiceModel.Http" Version="4.7.0" />
    <PackageReference Include="System.ServiceModel.Primitives" Version="4.7.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Fhi.HelseId.Common\Fhi.HelseId.Common.csproj" />
  </ItemGroup>

</Project>
